{
  "schemaVersion": 1,
  "id": "bingbingwahoo",
  "version": "${version}",

  "name": "Bing Bing Wahoo",
  "description": "Bringing the physics of Mario to Minecraft, one wahoo at a time.\nMade for ModFest 1.17.",
  "authors": [
    "Tropheus Jay",
    "InsaneASockJr"
  ],
  "contact": {
    "sources": "https://github.com/Ignoramuses/bing-bing-wahoo"
  },

  "license": "MIT",
  "icon": "assets/bingbingwahoo/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.ignoramuses.bingBingWahoo.BingBingWahoo"
    ],
    "client": [
      "net.ignoramuses.bingBingWahoo.BingBingWahooClient"
    ]
  },
  "mixins": [
    "bingbingwahoo.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": ">=${fabric_version}",
    "minecraft": ">=${minecraft_version}",
    "java": ">=${java_version}",
    "midnightlib": ">=${midnightlib_version}"
  },
  "recommends": {
    "modmenu": ">=3.0.0"
  }
}
